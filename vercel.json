{
  "version": 2,
  "builds": [
    { "src": "server.js", "use": "@vercel/node" },
    { "src": "*.html", "use": "@vercel/static" },
    { "src": "js/**", "use": "@vercel/static" },
    { "src": "textures/**", "use": "@vercel/static" },
    { "src": "sounds/**", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/socket.io/(.*)", "dest": "/server.js" },
    { "handle": "filesystem" },
    { "src": "/(.*)", "dest": "/server.js" }
  ]
}